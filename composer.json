{
    "name": "cccdl/dougong-pay",
    "type": "library",
    "description": "斗拱支付 SDK for PHP",
    "keywords": ["payment", "dougong", "sdk", "php"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "cccdl\\DougongPay\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4|^8.0",
